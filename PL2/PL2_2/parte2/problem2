(defproblem problem dominio
    (
    	(UBICACION deposito)
    	(UBICACION ubi1)

        (CONTENIDO medicina)
        (CONTENIDO comida)
    	
    	
    	(DRON d1)
    	(TRANSPORTADOR t1)

    	
        (dron-libre d1)

        (almacenamiento t1 0 10)


        (ubicacion-dron d1 deposito)
        (ubicacion-transportador t1 deposito)

        
        (ubicacion-numero-contenidos deposito 55 medicina)
        (ubicacion-numero-contenidos deposito 50 comida)

        (ubicacion-numero-contenidos ubi1 0 medicina)
        (ubicacion-numero-contenidos ubi1 0 comida)

        (ubicacion-necesita ubi1 20 medicina)
        (ubicacion-necesita ubi1 31 comida)
        (necesidad-de-recursos ubi1 51)
    )
    (
    	(enviar-todo)
    )
)
